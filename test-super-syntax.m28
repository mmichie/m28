# Test correct super syntax

(class Base
  (def __init__ (self name)
    (print f"Base.__init__ with {name}")
    (= self.name name)))

(class Derived (Base)
  (def __init__ (self name age)
    (print "Trying different super syntaxes...")
    
    # Try 1: ((super).__init__ name)
    (try
      (print "Try 1: ((super).__init__ name)")
      ((super).__init__ name)
      (except Exception e
        (print f"  Failed: {e}")))
    
    # Try 2: (super __init__ name)
    (try
      (print "Try 2: (super __init__ name)")
      (super __init__ name)
      (except Exception e
        (print f"  Failed: {e}")))
    
    # Try 3: ((super) __init__ name)
    (try
      (print "Try 3: ((super) __init__ name)")
      ((super) __init__ name)
      (except Exception e
        (print f"  Failed: {e}")))
    
    (= self.age age)))

(= d (Derived "Alice" 25))
(print f"Name: {d.name if (hasattr d 'name') else 'NOT SET'}")
; simple-tuple-test.m28 - a minimal test for tuple repetition

(print "Simple Tuple Repetition Test")

(= t (1, 2))
(print "Tuple:" t)

(= n 3)
(print "Number:" n)

(print "Will try to compute:" t "*" n)

; Test the core repetition operations
(try
  (= result (* t n))
  (print "Result:" result)
  (except Exception as e
    (print "Got exception:" e)))

(print "Test complete")
# Minimal decorator test

# Test 1: Define a simple function first
(def regular_func (x) (+ x 1))
print "Defined regular function"

# Test 2: Call it
(= r1 (regular_func 5))
print "Regular function result:" r1

# Test 3: Now try decorator
(@macro
 (def identity_macro (x) x))

print "Defined macro"

# Test 4: Check __macro__ attribute
(= has_attr (hasattr identity_macro "__macro__"))
print "Has __macro__ attribute:" has_attr

print "All tests passed!"

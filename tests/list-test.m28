(print "\n===== List Tests =====")

(print "\n-- List Creation --")
(= empty-list (list))
(= numbers (list 1 2 3 4 5))
(= mixed (list 1 "two" 3.0 True None))

(print "Empty list: ")
(print empty-list)
(print "Expected: []")

(print "Numbers list: ")
(print numbers)
(print "Expected: [1, 2, 3, 4, 5]")

(print "Mixed list: ")
(print mixed)
(print "Expected: [1, \"two\", 3.0, True, None]")

(print "\n-- List Access --")
(= nums (list 10 20 30 40 50))

(print "First element (index 0): ")
(print (nth nums 0))
(print "Expected: 10")

(print "Middle element (index 2): ")
(print (nth nums 2))
(print "Expected: 30")

(print "Last element (index 4): ")
(print (nth nums 4))
(print "Expected: 50")

(print "\n-- List Operations --")
(= list1 (list 1 2 3))
(= list2 (list 4 5 6))
(= combined (+ list1 list2))

(print "Combined lists: ")
(print combined)
(print "Expected: [1, 2, 3, 4, 5, 6]")

(print "\n-- List Iteration --")
(= sum 0)
(= nums (list 1 2 3 4 5))
(for num nums
  (= sum (+ sum num)))

(print "Sum of list elements: ")
(print sum)
(print "Expected: 15")

(print "\n===== Tests Complete =====")
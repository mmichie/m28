# Simplified Dot Notation Test
# Tests basic dot notation functionality with dictionaries

(print "=== Dot Notation Test ===")

# Create a simple dictionary
(= config (dict
  "app_name" "MyApp"
  "version" "1.0.3"
  "server" (dict
    "host" "localhost"
    "port" 8080
    "debug" True
  )
))

# Test basic dictionary access
(print "\nTest 1: Dictionary access")
(= app_name (get config "app_name"))
(print "App name:" app_name)
(= server (get config "server"))
(= server_host (get server "host"))
(print "Server host:" server_host)

# Test dictionary property access
(if (and (= app_name "MyApp") (= server_host "localhost"))
  (print "PASS: Dictionary access works")
  (print "FAIL: Dictionary access doesn't work"))

# Test nested dictionary access
(print "\nTest 2: Nested dictionary access")
(= port (get (get config "server") "port"))
(print "Server port:" port)
(if (= port 8080)
  (print "PASS: Nested dictionary access works")
  (print "FAIL: Nested dictionary access doesn't work"))

# Test list operations
(print "\nTest 3: List operations")
(= numbers (list 1 2 3 4 5))
(= length (len numbers))
(= first (nth numbers 0))
(= last (nth numbers 4))
(print "List length:" length)
(print "First item:" first)
(print "Last item:" last)
(if (and (= length 5) (= first 1) (= last 5))
  (print "PASS: List operations work")
  (print "FAIL: List operations don't work"))

(print "\n=== Dot Notation Test Complete ===")

# Return success status
(dict "status" "success")
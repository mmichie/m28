# Basic S-String Parser Tests

(print "=== S-String Parser Tests ===")

# Test 1: Basic s-string (should parse without errors)
(print "\nTest 1: Basic s-string parsing")
(= template s"(+ 1 2)")
(print "Parsed: " (type template))
(assert (== (type template) "list") "S-string should parse to list")

# Test 2: S-string with value interpolation
(print "\nTest 2: Value interpolation")
(= x 10)
(= code s"(+ {x} 1)")
(print "Parsed with interpolation: " code)

# Test 3: S-string with code interpolation
(print "\nTest 3: Code interpolation")
(= var 'myvar)
(= code2 s"(def {=var} 42)")
(print "Code interpolation: " code2)

# Test 4: Raw s-string (no interpolation)
(print "\nTest 4: Raw s-string")
(= raw rs"no {interpolation} here")
(print "Raw s-string: " raw)

# Test 5: Escape sequences
(print "\nTest 5: Escape sequences")
(= escaped s"literal \{ brace")
(print "Escaped: " escaped)

(print "\n=== All parser tests completed ===")
(print "Note: These tests only verify parsing, not evaluation")

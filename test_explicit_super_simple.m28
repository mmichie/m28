# Test explicit super
(class A
  (def __init__ (self)
    (= self.a "A")))

(class B (A)
  (def __init__ (self)
    # Test explicit super
    (print "Getting super B self...")
    (try
      (= s (super B self))
      (print "Got super:" s)
      (print "Calling init...")
      (s.__init__ self)
      (except Exception e
        (print "Error:" e)))
    (= self.b "B")))

(print "Creating B...")
(= b (B))
(print "b.a:" b.a)
(print "b.b:" b.b)
# Test inheritance with dict and list

(class Base
  (def __init__ (self name)
    (= self.name name)))

(class Derived (Base)
  (def __init__ (self name)
    (print "Calling super...")
    ((super Derived self).__init__ name)
    (print "Setting mylist...")
    (= self.mylist [])
    (print "List set!")))

(print "Creating derived...")
(= d (Derived "test"))
(print f"Success: {d.name}, list: {d.mylist}")
# Factorial function example using recursion

# Define factorial using recursion
(= factorial (lambda (n)
  (if (== n 0)
      1
      (* n (factorial (- n 1))))))

# Test with factorial of 5
(print "Factorial of 5:")
(print (factorial 5))

# Print factorials from 0 to 10
(print "Factorials from 0 to 10:")
(for i (range 11)
  (print (+ "Factorial of " (str i) ": " (str (factorial i)))))
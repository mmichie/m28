# Test nested dictionary access
(print "=== Testing Nested Dictionary Access ===")

# Create a nested dictionary
(= person {
  "name": "John",
  "age": 30,
  "address": {
    "street": "123 Main St",
    "city": "Anytown",
    "zip": 12345
  },
  "contact": {
    "email": "john@example.com",
    "phone": {
      "home": "555-1234",
      "work": "555-5678"
    }
  }
})

# Access nested attributes using dot notation
(print "Accessing nested dictionary attributes:")
(print "person.name:" (get person "name"))
(print "person.address.city:" (get (get person "address") "city"))
(print "person.contact.phone.home:" (get (get (get person "contact") "phone") "home"))

# Import test_module and test dot notation with imports
(import "modules/test_module")

# Access module dictionary with dot notation
(print "\nAccessing module dictionary attributes:")
(print "test_module.CONFIG.version:" (get (get test_module "CONFIG") "version"))
(print "test_module.CONFIG.author:" (get (get test_module "CONFIG") "author"))
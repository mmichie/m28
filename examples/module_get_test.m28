# Module access test using get
(print "=== Module Get Test ===")

# Import the module
(import "examples/basic_module")

# Use get to access properties
(print "\nModule properties:")
(print "Module name:" (get basic_module "__name__"))
(print "Module file:" (get basic_module "__file__"))
(print "TEST_VALUE:" (get basic_module "TEST_VALUE"))

# Try to call a function from the module
(print "\nCalling module function:")
(= test_func (get basic_module "test_func"))
(print "test_func(10):" (test_func 10))

(print "\n=== Test Completed ===")
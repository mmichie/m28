# Test dot notation with dictionaries

(def d (dict "name" "Alice" "age" 30))
(print "Dictionary:" d)

(def name (get d "name"))
(print "Name using get:" name)

# Use dot notation to get a property
(def name2 (dot d "name"))
(print "Name using dot notation:" name2)

# Verify it worked
(if (= name name2)
    (print "Success: Dictionary dot notation for property access works!")
    (print "Failure: Dictionary dot notation for property access failed!"))
# Test simple module import
(print "=== Testing Simple Module Import ===")

# Import module
(print "Importing simple_module...")
(import simple_module)
(print "simple_module type:" (type simple_module))

# Print module contents
(print "\nModule contents:")
(print simple_module)

# Print module type
(print "\nModule type:" (type simple_module))

# Try to access the module's variables
(print "\nAccessing module variables:")
(print "MODULE_NAME =" simple_module.MODULE_NAME)
(print "VALUE =" simple_module.VALUE)

# Try to call a module function
(print "\nCalling module function:")
(print "add(10, 20) =" (simple_module.add 10 20))
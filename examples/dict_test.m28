(def d (dict "name" "Alice" "age" 30 "city" "Wonderland"))
(print "Dictionary:" d)
(print "Keys:" (keys d))
(print "Values:" (values d))
(print "Items:" (items d))

(print "Name:" (get d "name"))
(print "Missing key with default:" (get d "country" "Unknown"))

(set d "country" "Wonderland")
(print "After adding country:" d)

(delete d "age")
(print "After deleting age:" d)

(print "Has name key:" (has-key d "name"))
(print "Has age key:" (has-key d "age"))

(def d2 (dict "job" "Engineer" "hobby" "Reading"))
(def merged (merge d d2))
(print "Merged dictionaries:" merged)
# Test for list assignment
# This file tests assigning lists with commas to variables in both formats

# Test variable assignment with = special form
(print "Testing = special form for list assignment...")
(= a [1, 2, 3])
(print "a =" a)  ; The value should be printed correctly

# Test nested lists
(= b [[1, 2], [3, 4]])
(print "b =" b)

# Test mixed elements
(= c [1, "two", 3.0])
(print "c =" c)

# Test accessing elements
(print "First element of a:" (nth a 0))
(print "Last element of a:" (nth a 2))

# Test concatenation
(= d (+ a [4, 5, 6]))
(print "d = a + [4, 5, 6] =" d)

# Test using def form too
(def test-list [7, 8, 9])
(print "test-list =" test-list)

# Test for comprehensions
(= squares [x for x [1, 2, 3, 4, 5]])
(print "squares =" squares)

(print "All tests completed successfully!")
# Test super() in detail

(class Base
  (def __init__ (self name)
    (print f"Base.__init__ called with {name}")
    (= self.name name)))

(class Derived (Base)
  (def __init__ (self name)
    (print "About to call super...")
    ((super Derived self).__init__ name)
    (print "Super called successfully")))

(print "Creating derived instance...")
(= d (Derived "test"))
(print f"Created: {d.name}")